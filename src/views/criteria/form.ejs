<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label>Name</label>
            <input type="text" name="name" class="form-control" placeholder="Name" value="<%= value.name%>" required />
        </div>
    </div>

    <div class="col-md-12">
        <div class="form-group">
            <label>Bobot</label>
            <input type="number" step="0.01" min="0" name="bobot" class="form-control" placeholder="Bobot" value="<%= value.bobot%>" required />
        </div>
    </div>
</div>
